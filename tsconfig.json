{
  "compilerOptions": {
      "moduleResolution": "node",
      "module": "ESNext",
      "types": [
          "node",
      ],
      "target": "es2022",
      "allowSyntheticDefaultImports": true,
      "allowImportingTsExtensions": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "experimentalDecorators": true,
      "noImplicitAny": false,
      "strict": false
  },
}